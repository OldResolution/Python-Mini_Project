{
  "QMainWindow": [
    {
      "tittle": "Login and signup",
      "icon": ":/font_awesome_brands/icons/font_awesome/brands/windows.png",
      "frameless": true,
      "transluscentBg": true,
      "navigation": [
        {
          "close": "close_window_button",
          "moveWindow": "header"
        }
      ]
    }
  ],
  "QSettings": [
    {
      "ThemeSettings": [
        {
          "CustomTheme": [
            {
              "Background-color": "#F5F5F5",
              "Text-color": "#333333",
              "Accent-color": "#008080",
              "Icons-color": "#333333",
              "Theme-name": "Modern Teal Slate",
              "Default-Theme": true
            }
          ]
        }
      ],
      "AppSettings": {
        "OrginizationName": "Modern UI",
        "ApplicationName": "Modern UI Company",
        "OrginizationDormain": "Modern UI.org"
      }
    }
  ],
  "QCustomQStackedWidget": [
    {
      "name": "stackedWidget",
      "navigation": [
        {
          "navigationButtons": [
            {
              "to_signup": "welcome_signup_pg",
              "to_login": "welcome_login_pg"
            }
          ]
        }
      ],
      "transitionAnimation": [
        {
          "slide": [
            {
              "active": true,
              "duration": 1000,
              "direction": "horizontal",
              "easingCurve": "Linear"
            }
          ]
        }
      ]
    },
    {
      "name": "stackedWidget_2",
      "navigation": [
        {
          "navigationButtons": [
            {
              "to_signup": "signup_pg",
              "to_login": "login_pg"
            }
          ]
        }
      ],
      "transitionAnimation": [
        {
          "slide": [
            {
              "active": true,
              "duration": 1000,
              "direction": "vertical",
              "easingCurve": "Linear"
            }
          ]
        }
      ]
    }
  ],
    "QCustomSlideMenu": [
        {
            "name": "notification_slide",
            "defaultSize": [
                {
                    "width": 600,
                    "height": 40
                }
            ],
            "collapsedSize": [
                {
                    "width": 600,
                    "height": 0
                }
            ],
            "expandedSize": [
                {
                    "width": 600,
                    "height": "auto"
                }
            ],
            "toggleButton": [
              {
                "buttonName": "close_notification"
              }
            ],
            "menuTransitionAnimation": [
                {
                    "animationDuration": 500,
                    "animationEasingCurve": "Linear"
                }
            ]
        }
    ]
}